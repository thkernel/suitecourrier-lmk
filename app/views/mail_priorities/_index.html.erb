<% @mail_priorities.each do |mail_priority| %>
  <tr>
    
    <td><%= mail_priority.name %></td>
    
    
    <td>
        <div class="action-buttons">
            <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'),  edit_mail_priority_path(mail_priority), title: "Modifier", remote: true %>




<%= link_to raw('<i class="far fa-trash-alt"></i>
'), mail_priority_delete_path(mail_priority), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>

  </tr>
<% end %>