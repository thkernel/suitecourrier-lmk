$("ul.errors").html("");
<% if @tenant.errors.any? %>
  <% @tenant.errors.full_messages.each do |message| %>
 
    $("ul.errors").append($('<div class="c-alert c-alert--danger" > ').html("<%= message.html_safe %> "));
  <% end %>
<% else %>
  $(".tenants").html("<%= escape_javascript(render 'index') %>");
  $("#tenant-modal").modal("hide");
 
<% end %>